a = float(input())
b = float(input())
c = input()
if c == '+':
    print(a + b)
elif c == '-':
    print(a - b)
elif c == '/' and b != 0.0:
    print(a / b)
elif c == '/':
    print('Деление на 0!')
elif c == '*':
    print(a * b)
elif c == 'mod' and b != 0.0:
    print(a % b)
elif c == 'mod':
    print('Деление на 0!')
elif c == 'pow':
    print(a ** b)
elif c == 'div' and b != 0.0:
    print(a // b)
elif c == 'div':
    print('Деление на 0!')
